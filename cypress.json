{
  "video": false,
  "viewportWidth": 1920,
  "viewportHeight": 1080,
  "chromeWebSecurity": false,
  "pageLoadTimeout": 15000,
  "defaultCommandTimeout": 30000,
  "responseTimeout": 30000,
  "requestTimeout": 30000,
  "downloadsFolder": "cypress/downloads",



  "reporter": "cypress-multi-reporters",
